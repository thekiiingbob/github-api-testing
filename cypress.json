{
  "baseUrl": "https://api.github.com",
  "env": { "github_token": "PUT_YOUR_TOKEN_HERE" },
  "reporter": "cypress-multi-reporters",
  "reporterOptions": {
    "reporterEnabled": "mochawesome",
    "mochawesomeReporterOptions": {
      "reportDir": "cypress/results/",
      "quiet": true,
      "overwrite": false,
      "html": false,
      "json": true
    }
  },
  "video": false
}
